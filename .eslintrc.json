{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "always"
      }
    ],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": ["gulpfile.js", "webpack.config.js", "gulp/**/*"]}]
  }
}
